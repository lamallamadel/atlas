{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "install-browsers": "playwright install",
    "e2e": "playwright test",
    "e2e:fast": "playwright test -c playwright.fast.config.ts",
    "e2e:fast:headed": "playwright test -c playwright.fast.config.ts --headed",
    "e2e:fast:ui": "playwright test -c playwright.fast.config.ts --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:ui": "playwright test --ui",
    "e2e:postgres": "playwright test -c playwright-postgres-mock.config.ts",
    "e2e:h2-keycloak": "playwright test -c playwright-h2-keycloak.config.ts",
    "e2e:postgres-keycloak": "playwright test -c playwright-postgres-keycloak.config.ts",
    "e2e:full": "npm run e2e && npm run e2e:postgres && npm run e2e:h2-keycloak && npm run e2e:postgres-keycloak",
    "e2e:a11y": "playwright test -c playwright.config.ts --grep @a11y",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/frontend/stats.json",
    "a11y": "node accessibility-audit.js",
    "a11y:lighthouse": "npx lighthouse http://localhost:4200 --only-categories=accessibility --view",
    "a11y:pre-commit": "node scripts/a11y-pre-commit.js",
    "lighthouse:ci": "lhci autorun",
    "prepare": "cd .. && husky install frontend/.husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/cdk": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/material": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@fullcalendar/angular": "^6.1.9",
    "@fullcalendar/core": "^6.1.9",
    "@fullcalendar/daygrid": "^6.1.9",
    "@fullcalendar/timegrid": "^6.1.9",
    "@fullcalendar/list": "^6.1.9",
    "@fullcalendar/interaction": "^6.1.9",
    "@fullcalendar/icalendar": "^6.1.9",
    "angular-oauth2-oidc": "^16.0.0",
    "chart.js": "^4.4.0",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.2",
    "ng2-charts": "^5.0.3",
    "papaparse": "^5.4.1",
    "rxjs": "~7.8.0",
    "shepherd.js": "^11.2.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular-eslint/builder": "^16.3.1",
    "@angular-eslint/eslint-plugin": "^16.3.1",
    "@angular-eslint/eslint-plugin-template": "^16.3.1",
    "@angular-eslint/schematics": "^16.3.1",
    "@angular-eslint/template-parser": "^16.3.1",
    "@angular/cli": "^16.2.0",
    "@angular/compiler-cli": "^16.2.0",
    "@playwright/test": "^1.57.0",
    "@types/jasmine": "~4.3.0",
    "@types/jspdf": "^2.0.0",
    "@types/node": "^20.11.0",
    "@types/papaparse": "^5.3.14",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.1",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "axe-core": "^4.8.3",
    "axe-playwright": "^2.0.1",
    "@axe-core/playwright": "^4.8.3",
    "lighthouse": "^11.4.0",
    "@lhci/cli": "^0.13.0",
    "husky": "^8.0.3",
    "typescript": "~5.1.3",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}
