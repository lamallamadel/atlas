{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "start:fr": "ng serve --configuration=fr --proxy-config proxy.conf.json",
    "start:en": "ng serve --configuration=en --proxy-config proxy.conf.json",
    "start:es": "ng serve --configuration=es --proxy-config proxy.conf.json",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "build:prod:fr": "ng build --configuration production,fr",
    "build:prod:en": "ng build --configuration production,en",
    "build:prod:es": "ng build --configuration production,es",
    "build:prod:all": "npm run build:prod:fr && npm run build:prod:en && npm run build:prod:es",
    "build:analyze": "ng build --configuration production --stats-json && npm run analyze:bundle",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "extract-i18n": "ng extract-i18n --output-path src/locale",
    "install-browsers": "playwright install",
    "e2e": "playwright test",
    "e2e:fast": "playwright test -c playwright.fast.config.ts",
    "e2e:fast:headed": "playwright test -c playwright.fast.config.ts --headed",
    "e2e:fast:ui": "playwright test -c playwright.fast.config.ts --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:ui": "playwright test --ui",
    "e2e:postgres": "playwright test -c playwright-postgres-mock.config.ts",
    "e2e:h2-keycloak": "playwright test -c playwright-h2-keycloak.config.ts",
    "e2e:postgres-keycloak": "playwright test -c playwright-postgres-keycloak.config.ts",
    "e2e:full": "npm run e2e && npm run e2e:postgres && npm run e2e:h2-keycloak && npm run e2e:postgres-keycloak",
    "e2e:a11y": "playwright test -c playwright.config.ts --grep @a11y",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/frontend/stats.json",
    "analyze:bundle": "webpack-bundle-analyzer dist/frontend/stats.json",
    "analyze:source": "source-map-explorer dist/frontend/**/*.js",
    "a11y": "node accessibility-audit.js",
    "a11y:lighthouse": "npx lighthouse http://localhost:4200 --only-categories=accessibility --view",
    "a11y:pre-commit": "node scripts/a11y-pre-commit.js",
    "lighthouse:ci": "lhci autorun",
    "prepare": "cd .. && husky install frontend/.husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "cap:init": "cap init",
    "cap:add:ios": "cap add ios",
    "cap:add:android": "cap add android",
    "cap:sync": "cap sync",
    "cap:sync:ios": "cap sync ios",
    "cap:sync:android": "cap sync android",
    "cap:open:ios": "cap open ios",
    "cap:open:android": "cap open android",
    "cap:copy": "cap copy",
    "cap:update": "cap update",
    "build:mobile": "ng build --configuration production && cap sync",
    "build:ios": "ng build --configuration production && cap sync ios && cap open ios",
    "build:android": "ng build --configuration production && cap sync android && cap open android"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/cdk": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/localize": "^16.2.0",
    "@angular/material": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@capacitor/android": "^5.5.1",
    "@capacitor/app": "^5.0.6",
    "@capacitor/biometric": "^0.1.0",
    "@capacitor/browser": "^5.1.0",
    "@capacitor/calendar": "^5.0.0",
    "@capacitor/core": "^5.5.1",
    "@capacitor/device": "^5.0.6",
    "@capacitor/filesystem": "^5.1.4",
    "@capacitor/haptics": "^5.0.6",
    "@capacitor/ios": "^5.5.1",
    "@capacitor/keyboard": "^5.0.6",
    "@capacitor/network": "^5.0.6",
    "@capacitor/push-notifications": "^5.1.0",
    "@capacitor/screen-reader": "^5.0.6",
    "@capacitor/share": "^5.0.6",
    "@capacitor/splash-screen": "^5.0.6",
    "@capacitor/status-bar": "^5.0.6",
    "@fullcalendar/angular": "^6.1.9",
    "@fullcalendar/core": "^6.1.9",
    "@fullcalendar/daygrid": "^6.1.9",
    "@fullcalendar/timegrid": "^6.1.9",
    "@fullcalendar/list": "^6.1.9",
    "@fullcalendar/interaction": "^6.1.9",
    "@fullcalendar/icalendar": "^6.1.9",
    "angular-oauth2-oidc": "^16.0.0",
    "chart.js": "^4.4.0",
    "d3": "^7.8.5",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.2",
    "lottie-web": "^5.12.2",
    "ng2-charts": "^5.0.3",
    "papaparse": "^5.4.1",
    "rxjs": "~7.8.0",
    "shepherd.js": "^11.2.0",
    "sockjs-client": "^1.6.1",
    "stompjs": "^2.3.3",
    "tslib": "^2.3.0",
    "xlsx": "^0.18.5",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular-eslint/builder": "^16.3.1",
    "@angular-eslint/eslint-plugin": "^16.3.1",
    "@angular-eslint/eslint-plugin-template": "^16.3.1",
    "@angular-eslint/schematics": "^16.3.1",
    "@angular-eslint/template-parser": "^16.3.1",
    "@angular/cli": "^16.2.0",
    "@angular/compiler-cli": "^16.2.0",
    "@capacitor/assets": "^3.0.1",
    "@capacitor/cli": "^5.5.1",
    "@playwright/test": "^1.57.0",
    "@types/d3": "^7.4.3",
    "@types/jasmine": "~4.3.0",
    "@types/jspdf": "^2.0.0",
    "@types/node": "^20.11.0",
    "@types/papaparse": "^5.3.14",
    "@types/sockjs-client": "^1.5.4",
    "@types/stompjs": "^2.3.9",
    "@types/xlsx": "^0.0.36",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.1",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "axe-core": "^4.8.3",
    "axe-playwright": "^2.0.1",
    "@axe-core/playwright": "^4.8.3",
    "lighthouse": "^11.4.0",
    "@lhci/cli": "^0.13.0",
    "husky": "^8.0.3",
    "typescript": "~5.1.3",
    "webpack-bundle-analyzer": "^4.10.1",
    "source-map-explorer": "^2.5.3",
    "workbox-webpack-plugin": "^7.0.0",
    "@storybook/angular": "^7.6.7",
    "@storybook/addon-links": "^7.6.7",
    "@storybook/addon-essentials": "^7.6.7",
    "@storybook/addon-interactions": "^7.6.7",
    "@storybook/addon-a11y": "^7.6.7",
    "@storybook/addon-viewport": "^7.6.7",
    "@storybook/addon-measure": "^7.6.7",
    "@storybook/addon-outline": "^7.6.7",
    "@storybook/blocks": "^7.6.7",
    "storybook": "^7.6.7"
  }
}
